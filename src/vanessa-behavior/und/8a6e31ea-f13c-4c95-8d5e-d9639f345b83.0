		Команда = ИмяВременногоBat;
		Статус = ВыполнитьКомандуОСБезПоказаЧерногоОкна(Команда);
		Если Статус <> 0 Тогда
			ВызватьИсключение ""Не смог создать сервисные базы! Статус=""+Статус; 
		КонецЕсли;	 
	КонецЕсли;	 
