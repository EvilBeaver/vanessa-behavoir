	ЗТ = Новый ЗаписьТекста(ИмяФайлаВерсииИсходников,""UTF-8"",,Истина); 
	ЗТ.ЗаписатьСтроку(""Версия=""+Версия); 
	
	ЗТ.Закрыть();
	
