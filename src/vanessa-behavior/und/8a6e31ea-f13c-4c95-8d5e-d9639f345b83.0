					Изменение = ""элемент формы с именем """""" + ЭлемСтало.Имя + """""" стал равен """""" + ЭлемСтало.ПредставлениеДанных + """""""";
					
					Если Стр = """" Тогда
						Стр = Стр + ""	Тогда "" + Изменение + Символы.ПС;
					Иначе	
						Стр = Стр + ""	И     "" + Изменение + Символы.ПС;
					КонецЕсли;	 
				Иначе	
					Если НЕ ЭлемБыло.Свойство(Ключ) Тогда
